{
  "name": "@ckboost/client",
  "version": "0.1.0",
  "description": "TypeScript/JavaScript SDK for dApps to integrate CKBoost - Fast ck-token transactions on Internet Computer through liquidity pools",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist",
    "test": "echo \"No tests yet\"",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "ckboost",
    "ck-tokens",
    "ckbtc",
    "internet-computer",
    "icp",
    "dapp-integration",
    "transaction-acceleration",
    "sdk",
    "typescript"
  ],
  "author": "CKBoost Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ckboost/ckboost-packages",
    "directory": "packages/client"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "tsup": "^8.0.0"
  },
  "dependencies": {
    "@dfinity/agent": "^0.21.0",
    "@dfinity/candid": "^0.21.0",
    "@dfinity/principal": "^0.21.0",
    "axios": "^1.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
} 